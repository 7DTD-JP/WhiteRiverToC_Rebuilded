	<!-- NG 082523 -->
	<append xpath="/ui_display_info/item_display">
		<item_display_info display_type="h7sb_carryCapacity" display_group="modAttire">
			<display_entry name="dCarryCapacity" title_key="statCarryCapacity" display_leading_plus="true"/>
