			<!-- <crafting_category name="WRC_Traders" icon="ui_game_symbol_map_trader" display_name="lblWRC_Traders"/> -->
			<crafting_category name="WRC_Crates" icon="ui_game_symbol_twitch_drop" display_name="lblWRC_Crates"/>
			<crafting_category name="WRC_Quests" icon="ui_game_symbol_quest" display_name="lblWRC_Quests"/>
		</crafting_category_list>
	</append>
